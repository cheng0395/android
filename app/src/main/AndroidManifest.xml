<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.maoyan">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activity.SeatActivity"
            android:theme="@style/fullScreen"></activity>
        <activity
            android:name=".Activity.ReserveActivity"
            android:theme="@style/fullScreen" />
        <activity
            android:name=".Activity.ToBeShownDetailActivity"
            android:theme="@style/fullScreen" />
        <activity
            android:name=".Activity.HotDetailActivity"
            android:theme="@style/fullScreen" />
        <activity
            android:name=".Activity.RegisterActivity"
            android:theme="@style/fullScreen" />
        <activity
            android:name=".Activity.LoginActivity"
            android:theme="@style/fullScreen">

        </activity>
        <activity
            android:name=".Activity.StartActivity"
            android:theme="@style/fullScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.MainActivity"
            android:theme="@style/fullScreen">

        </activity>
    </application>

</manifest>